<p> Viewing Party Manager </p>

<p> All Users </p>
<% @users.each do |user| %>
  <%= link_to user.name, user_path(user.id) %> <br>
<% end %>


<% if session[:user_id] %>
  <%= button_to "Log Out", logout_path, method: :get %>
<% else %>
  <%= button_to "Create new user", register_user_path, method: :get %>
  <%= link_to "Log In", login_path %>
<% end %>

