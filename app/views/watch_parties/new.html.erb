<%= form_with url: "/users/#{@user.id}", local: true  do |f| %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :duration %>
  <%= f.text_field :duration %>
  <%= f.label :start_time %>
  <%= f.text_field :start_time %>
  <%= f.label :movie_id %>
  <%= f.text_field :movie_id %>

  <%= f.submit %>
<% end %>